<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DoctorViewFragment"
    android:background="#D1D1D6">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="10dp"
        android:background="@color/white">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:layout_margin="10dp">
            <ImageView
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:src="@drawable/img"
                android:layout_gravity="center"
                android:layout_margin="10dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/dlable_fname"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="First Name"
                android:textStyle="bold"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                />
            <TextView
                android:id="@+id/dtxt_firstn"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/dlabel_lname"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="Last Name"
                android:textStyle="bold"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                />
            <TextView
                android:id="@+id/dtxt_lastn"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/dlabel_email"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="Email"
                android:textStyle="bold"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                />
            <TextView
                android:id="@+id/dtxt_email"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/dlabel_mobile"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="Mobile"
                android:textStyle="bold"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                />
            <TextView
                android:id="@+id/dtxt_phone"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/dlabel_date"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="Date"
                android:textSize="16sp"
                android:padding="8dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                />
            <TextView
                android:id="@+id/dtxt_dob"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/dlabel_sex"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="Sex"
                android:textSize="16sp"
                android:padding="8dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                />
            <TextView
                android:id="@+id/dtxt_sex"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/dlabel_city"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="City"
                android:textSize="16sp"
                android:textStyle="bold"
                android:padding="8dp"
                android:textColor="@color/black"
                />
            <TextView
                android:id="@+id/dtxt_city"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/dlabel_nationality"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="Nationality"
                android:textSize="16sp"
                android:textStyle="bold"
                android:padding="8dp"
                android:textColor="@color/black"
                />
            <TextView
                android:id="@+id/dtxt_nationality"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">

            <TextView
                android:id="@+id/dlabel_languages"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:padding="8dp"
                android:text="Languages"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/dtxt_languages"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>
        </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="10dp">
                <TextView
                    android:id="@+id/dlabel_zip"
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="times"
                    android:text="Zip"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:padding="8dp"
                    android:textColor="@color/black"
                    />
                <TextView
                    android:id="@+id/dtxt_zip"
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="times"
                    android:textSize="16sp"
                    android:padding="8dp"
                    android:textColor="@color/black"
                    android:layout_marginStart="20dp"/>


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="10dp">
                <TextView
                    android:id="@+id/dlabel_specilization"
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="times"
                    android:text="Specilization"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:padding="8dp"
                    android:textColor="@color/black"
                    />
                <TextView
                    android:id="@+id/dtxt_specilization"
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="times"
                    android:textSize="16sp"
                    android:padding="8dp"
                    android:textColor="@color/black"
                    android:layout_marginStart="20dp"/>


            </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/label_address"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="Address"
                android:textSize="16sp"
                android:textStyle="bold"
                android:padding="8dp"
                android:textColor="@color/black"
                />
            <TextView
                android:id="@+id/dtxt_address"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <TextView
                android:id="@+id/label_phistory"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="Patient History"
                android:textSize="16sp"
                android:textStyle="bold"
                android:padding="8dp"
                android:textColor="@color/black"
                />
            <TextView
                android:id="@+id/dtxt_phistory"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:textSize="16sp"
                android:padding="8dp"
                android:textColor="@color/black"
                android:layout_marginStart="20dp"/>


        </LinearLayout>











    </LinearLayout>

</ScrollView>